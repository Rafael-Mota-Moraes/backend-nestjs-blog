@baseurl = http://localhost:3000 
@token = {{authLogin.response.body.accessToken}}
@postUuid = {{createPostOwned.response.body.id}}

GET {{baseurl}}/outra-rota 

### Auth login

# @name authLogin
POST {{baseurl}}/auth/login
Content-Type: application/json

{
  "email": "email@email.com",
  "password": "user123"
}

### Read one user

# @name readOneUser
GET {{baseurl}}/user/123
Authorization: Bearer {{token}}


### Create user

# @name createUser
POST {{baseurl}}/user
Content-Type: application/json

{
  "email": "email@email.com",
  "password": "user123",
  "name": "user"
}

### Update user

# @name updateUser
PATCH  {{baseurl}}/user/me
Content-Type: application/json
Authorization: Bearer {{token}}

{
  "email": "email@email.com",
  "name": "user"
}


### Update user password

# @name createUserPassword
PATCH  {{baseurl}}/user/me/password
Content-Type: application/json
Authorization: Bearer {{token}}

{
  "currentPassword": "user123",
  "newPassword": "user123"
}

### Read one user

# @name readOneUser
GET  {{baseurl}}/user/me/
Content-Type: application/json
Authorization: Bearer {{token}}

### Delete one user 

# @name readOneUser
DELETE  {{baseurl}}/user/me/
Content-Type: application/json
Authorization: Bearer {{token}}


### Create post owned
# @name createPostOwned
POST {{baseurl}}/post/me
Content-Type: application/json
Authorization: Bearer {{token}}

{
  "title": "DELETE Título do post",
  "excerpt": "Excerto Excerto",
  "content": "Conteúdo"
}

### Read one post owned
# @name readOnePostOwned

GET {{baseurl}}/post/me/{{postUuid}}
Authorization:  Bearer {{token}}


### Read all post owned
# @name readAllPostOwned

GET {{baseurl}}/post/me
Authorization:  Bearer {{token}}

### Update one post owned
# @name updateOnePostOwned

PATCH {{baseurl}}/post/me/{{postUuid}}
Content-Type:  application/json
Authorization: Bearer {{token}}

{
  "title": "NOVO - Título do post",
  "excerpt": "NOVO - Excerto",
  "content": "NOVO - Conteúdo",
  "published": true
}

### Delete one post owned
# @name deleteOnePostOwned

DELETE {{baseurl}}/post/me/{{postUuid}}
Authorization: Bearer {{token}}

